clickhouse:
  replace: true
  profiles:
    default:
      max_memory_usage: 10000000000
      use_uncompressed_cache: 0
      load_balancing: in_order
      log_queries: 1
      readonly: 1
    analytics:
      max_memory_usage: 10000000000
      use_uncompressed_cache: 0
      load_balancing: in_order
      log_queries: 1
      readonly: 0
  users:
    default:
      access_management: 1
      named_collection_control: 1
      show_named_collections: 1
      show_named_collections_secrets: 1
      profile: default
#      networks:
#        ip: "::/0"
      quota: default
    admin:
      password: admin
      grants:
        query: grant all on *.* to admin with grant option;
      profile: analytics
#      networks:
#        ip: "::"
      quota: default
    analytics_user:
      password: complex_password
      access_management: 1
#      grants:
#        query: grant all on *.* to analytics_user with grant option;
      profile: default
      networks:
        ip: 0.0.0.0/0
      quota: default
  quotas:
    default:
      interval:
        duration: 3600
        queries: 0
        errors: 0
        result_rows: 0
        read_rows: 0
        execution_time: 0
